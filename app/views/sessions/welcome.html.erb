<h1>Welcome </h1>

<% if logged_in? %>
    <h1> Hola <%= current_user.username %></h1>
    <%= button_to 'Logout', '/users/new', method: :get %>
    <%= button_to 'Login', '/login', method: :get %>
    
<% else %>
    <%= button_to 'Login', '/login', method: :get %>
    <%= button_to 'Sign Up', '/users/new', method: :get %>
<% end %>